      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "co.carminido.eunstagram",
      "versionCode": "1.0.0"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#00000000"
    }
  }
}
